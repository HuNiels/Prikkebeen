                <a class="dropdown-item text-dark text-nowrap border-top border-dark" style="cursor:pointer" onclick="insertTagAtSelection('RES_START_TIME')">Starttijd afspraak</a>
                <a class="dropdown-item text-dark text-nowrap border-top border-dark" style="cursor:pointer" onclick="insertTagAtSelection('RES_END_TIME')">Eindtijd afspraak</a>
                <a class="dropdown-item text-dark text-nowrap border-top border-dark" style="cursor:pointer" onclick="insertTagAtSelection('ADMIN_FIRST_NAME')">Admin naam</a>

            </div>
        </div>
    </div>
    <div>
        <input class="d-none" type="file" id="imageInput" accept="image/*" onchange="uploadFile(this, validateImage, 'image')" />
        <input class="d-none" type="file" id="videoInput" accept="video/*" onchange="uploadFile(this, validateVideo, 'video')" />
    </div>
    <div class="overflow-auto border border-dark w-100 mt-1 p-1" id="editField" contenteditable="true" >@Html.Raw(Model.CurrentPage.Content.RawHtml)</div>
    <textarea class="d-none" name="rawHtml" id="htmlField"></textarea>
    <input class="mt-1" onclick="prepareHtml()" type="submit" value="Opslaan" />
}

@section scripts {
    <script src="~/js/Edit.js" type="text/javascript" /></script>
}