                <a class="dropdown-item text-dark text-nowrap border-top border-dark" style="cursor:pointer" onclick="insertTagAtSelection('LAST_NAME')">Achternaam</a>
                <a class="dropdown-item text-dark text-nowrap border-top border-dark" style="cursor:pointer" onclick="insertTagAtSelection('DATE_OF_BIRTH')">Geboortedatum</a>
                <a class="dropdown-item text-dark text-nowrap border-top border-dark" style="cursor:pointer" onclick="insertTagAtSelection('HOUSE_ADDRESS')">Straat</a>
                <a class="dropdown-item text-dark text-nowrap border-top border-dark" style="cursor:pointer" onclick="insertTagAtSelection('HOUSE_NUMBER')">Huisnummer</a>
                <a class="dropdown-item text-dark text-nowrap border-top border-dark" style="cursor:pointer" onclick="insertTagAtSelection('POSTAL_CODE')">Postcode</a>
                <a class="dropdown-item text-dark text-nowrap border-top border-dark" style="cursor:pointer" onclick="insertTagAtSelection('CITY')">Stad</a>
                <a class="dropdown-item text-dark text-nowrap border-top border-dark" style="cursor:pointer" onclick="insertTagAtSelection('COUNTRY')">Land</a>
                <a class="dropdown-item text-dark text-nowrap border-top border-dark" style="cursor:pointer" onclick="insertTagAtSelection('TELEPHONE_NUMBER')">Telefoonnummer</a>
